    <form method="post" id="doiMKForm">
        <div id="ModalDoiMK" class="modal fade" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header" style="padding:5px">
                        <button type="button" class="close" data-dismiss="modal">×</button>
                        <h4>Đổi mật khẩu</h4>
                    </div>

                    <div class="modal-body" style="padding:40px 50px;">
                        <div id="divthongbaoketqua">

                        </div>
                        <table class="table-responsive">
                            <tbody>
                                <tr>
                                    <td width="25%">Mật khẩu cũ :</td>
                                    <td>
                                        <input type="password" class="form-control" required="" name="txtPW" id="txtPW" width="80%">
                                    </td>
                                </tr>
                                <tr>
                                    <td>Mật khẩu mới :</td>
                                    <td><input type="password" class="form-control" required="" name="txtPW1" id="txtPW1" width="80%"></td>
                                </tr>
                                <tr>
                                    <td>Nhập lại mật khẩu mới :</td>
                                    <td><input type="password" class="form-control" required="" name="txtPW2" id="txtPW2" width="80%"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary btn-sm" id="btnUpdate">Cập nhật mật khẩu</button>
                        <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">Đóng</button>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <script src="~/Scripts/site/Sweetalert2/Sweetalert2.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/Scripts/site/Datatables.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>

    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
<script>
        function goBack() {
            window.history.back();
            location.reload();
        }
        $(document).ready(function () {
            // Intercept the form submission
            $("#doiMKForm").submit(function (e) {
                e.preventDefault(); // Prevent the form from submitting normally

                // Perform an AJAX request to submit the form data
                $.ajax({
                    type: "POST",
                    url: "/Home/DoiMK",
                    data: $(this).serialize(),
                    success: function (data) {
                        // Update the status in the modal
                        $("#divthongbaoketqua").html("<p class='text-center text-warning'>" + data.Status + "</p>");

                        $("#txtPW, #txtPW1, #txtPW2").val("");

                        if (!$("#ModalDoiMK").is(":visible")) {
                            $("#ModalDoiMK").modal("show");
                        }
                    },
                    error: function (xhr, status, error) {
                        // Handle the error case
                        console.error("AJAX request failed:", status, error);

                        // Update the status in the modal with an error message
                        $("#divthongbaoketqua").html("<p class='text-center text-warning'>" + data.Status + "</p>");
                        $("#txtPW, #txtPW1, #txtPW2").val("");

                        // Show the modal if it's not already shown
                        if (!$("#ModalDoiMK").is(":visible")) {
                            $("#ModalDoiMK").modal("show");
                        }
                    }
                });
            });
        });

</script>

